{
  "homepage": "./",
  "name": "win28-electron-react-2",
  "description": "Win28 Desktop App",
  "author": "Dandem Labs",
  "version": "0.2.0",
  "private": false,
  "main": "public/electron.js",
  "build": {
    "appId": "com.win28.app",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "publish": [{
      "provider": "github",
      "owner": "daryl-ddl",
      "repo": "win28-app-test"
    }]
  },
  "dependencies": {
    "@chakra-ui/react": "^1.5.1",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@stripe/react-stripe-js": "^1.4.0",
    "@stripe/stripe-js": "^1.13.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@woocommerce/woocommerce-rest-api": "^1.0.1",
    "electron-updater": "^4.3.8",
    "framer-motion": "^4",
    "react": "^17.0.2",
    "react-country-flag": "^2.3.0",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-start": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-build": "electron-builder",
    "electron-pack": "yarn build && yarn electron-build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^6.0.2",
    "cross-env": "^7.0.3",
    "electron": "^12.0.4",
    "electron-builder": "^22.10.5",
    "wait-on": "^5.3.0"
  }
}
